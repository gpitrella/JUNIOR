{
  "name": "chained-function",
  "version": "0.5.0",
  "description": "Chained function calls",
  "main": "lib/index.js",
  "scripts": {
    "prepublish": "npm run lint && npm test && npm run build",
    "build": "babel --out-dir ./lib ./src",
    "test": "./babel-tap --coverage test/*.js",
    "lint": "eslint ./src",
    "lint:fix": "eslint --fix ./src",
    "coveralls": "./babel-tap --coverage --coverage-report=text-lcov test/*.js | node_modules/.bin/coveralls"
  },
  "author": "Cheton Wu <cheton@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cheton/chained-function.git"
  },
  "keywords": [
    "chained",
    "function"
  ],
  "devDependencies": {
    "babel-cli": "^6.23.0",
    "babel-core": "^6.23.1",
    "babel-eslint": "^7.1.1",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-stage-0": "^6.22.0",
    "coveralls": "^2.11.16",
    "eslint": "^3.15.0",
    "eslint-config-trendmicro": "^0.5.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.10.0",
    "tap": "^10.1.1"
  },
  "nyc": {
    "exclude": [
      "test/index.js"
    ]
  }
}
